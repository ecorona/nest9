{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Nest Framework",
      "runtimeArgs": ["--no-lazy", "-r", "ts-node/register", "--inspect"],
      "sourceMaps": true,
      "args": ["${workspaceFolder}/src/main.ts"],
      "autoAttachChildProcesses": true,
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
